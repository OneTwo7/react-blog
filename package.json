{
  "name": "blog",
  "version": "0.0.1",
  "description": "A simple blog app based on node, react and express.",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run lint:watch\"",
    "server": "node server.js",
    "lint": "esw webpack.config.* src",
    "lint:watch": "npm run lint -- --watch",
    "test": "ava test --verbose",
    "test:watch": "npm run test -- --watch"
  },
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/test-browser-env.js"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/OneTwo7/react-blog.git"
  },
  "author": "onetwo7",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OneTwo7/react-blog/issues"
  },
  "homepage": "https://github.com/OneTwo7/react-blog#readme",
  "dependencies": {
    "express": "^4.16.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-notifications": "^1.4.3",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.7",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.13.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-watch": "^3.1.3",
    "file-loader": "^1.1.11",
    "jsdom": "^11.5.1",
    "node-sass": "^4.7.2",
    "prop-types": "^15.6.0",
    "redux-mock-store": "^1.3.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.19.1",
    "url-loader": "^1.0.1",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.21.0"
  }
}
